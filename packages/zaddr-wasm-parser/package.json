{
  "name": "zaddr-wasm-parser",
  "version": "0.2.0",
  "type": "module",
  "description": "Zcash address parser via WASM",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "pkg"
  ],
"scripts": {
  "build": "tsc && npm run copy-pkg",
  "copy-pkg": "copyfiles -u 1 ../pkg/**/* .",
  "demo": "npm run build && node dist/demo.js",
  "demo-dev": "USE_SRC=1 tsx demo.ts",
  "fuzz": "tsx src/fuzz.test.ts",
  "test": "vitest"
},
  "keywords": [
    "zcash",
    "wasm",
    "unified address",
    "parser"
  ],
  "author": "ruzcash",
  "license": "MIT",
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "vitest": "^1.4.0",
    "tsx": "^4.19.3"
  }
}
